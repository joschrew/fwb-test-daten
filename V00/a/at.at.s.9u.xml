    <body>
      <entry xml:id="at.s.9u" sortKey="at.at.s.9u">
        <form type="lemma">
          <orth>at,</orth>
        </form>
        
        <dictScrap rend="artkopf" xml:space="preserve">
        
        <etym xml:space="preserve"> Etymologie und Wortart unsicher.</etym>
        
      </dictScrap>
        
        <!-- Bedeutung  -->
        <sense>
          <!-- Definition -->
          <def>laut <bibl xml:space="preserve"><name n="492" cert="high">Henisch</name> eine kindersprachliche Interjektion</bibl>; im Beleg von H. Salat könnte die Bed. ›Essen‹ angesetzt werden.</def>
          
          <!-- Quellen -->
          <dictScrap rend="cit">
            <cit>
              <bibl xml:space="preserve"><name n="492" cert="high">Henisch</name> <citedRange n="492" facs="135" rend="export" target="https://www.digitale-sammlungen.de/de/view/bsb10495873?page=84">135</citedRange> (<region key="schwäb.">Augsb.</region> <date from="1616" to="1616" confidence="1">1616</date>)</bibl>
              <quote xml:space="preserve"><oRef norm="at" type="oVar.lem" rend="automatic" n="67">At</oRef> / ein woͤrtlin der Kinder / so speiß begeren / von as, esca. </quote>
            </cit>
            
            <cit>
              <bibl xml:space="preserve"><name n="724" cert="high">Bächtold</name>, H. Salat <citedRange n="724" facs="47,28" rend="export" target="https://archive.org/details/hanssalateinsch00baecgoog/page/n67">47, 28</citedRange> (<region key="halem.">Luzern</region> <date from="1534" to="1534" confidence="1">1534</date>)</bibl>
              <quote xml:space="preserve">ward er krank, dass man meint, er hetts blatt; dann im moht kein aat werden.</quote>
            </cit>
            
          </dictScrap>
        </sense>
      </entry>
